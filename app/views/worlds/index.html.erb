<p id="notice"><%= notice %></p>

<h1>Worlds</h1>


 

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Made By</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @worlds.each do |world| %>
      <tr>
        <td><%= world.name %></td>
         <td><%= world.user %></td>
        <td><%= link_to 'Show', world %></td>
         </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New World', new_world_path %>
<br>
<a href="/search">Or would you rather search?</a>
